<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>Greenstack.GameStats</RootNamespace>

    <PackageId>Greenstack.GameStats</PackageId>
    <Version>1.0.0</Version>
    <Authors>Greenstack</Authors>
    <Description>A powerful and flexible library for monitoring, managing, and analyzing game statistics. Perfect for developers seeking robust solutions for games.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/greenstack/GameStats</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

</Project>
